<section id="sleepid-quiz-section-{{ section.id }}" class="sleepid_quiz_section">
  <div class="sleepid_section_surface" data-sleepid-surface>
    <div class="sleepid_threads" aria-hidden="true"></div>
    <div class="sleepid_fingerprint" aria-hidden="true"></div>
    <div class="sleepid_quiz_card">
      <div class="sleepid_card_shimmer" aria-hidden="true"></div>
      <div class="sleepid_card_inner">
        <button type="button" class="sleepid_close_btn" data-sleepid-close aria-label="Close SleepID quiz">
          <span aria-hidden="true">×</span>
        </button>
        <div class="sleepid_logo_wrap">
          <img class="sleepid_logo" src="{{ section.settings.logo_url | escape }}" alt="{{ shop.name }} logo" />
          <div class="sleepid_logo_shimmer" aria-hidden="true"></div>
        </div>
        <div class="sleepid_progress_region" aria-live="polite">
          <div class="sleepid_progress_meta">
            <p class="sleepid_progress_label">SleepID Rhythm Tracker</p>
            <div class="sleepid_progress_bar">
              <div class="sleepid_progress_fill" data-sleepid-progress-fill style="width:0%"></div>
            </div>
          </div>
          <div class="sleepid_progress_ring" data-sleepid-progress-ring>
            <svg viewBox="0 0 120 120" class="sleepid_progress_svg" role="img" aria-hidden="true">
              <circle cx="60" cy="60" r="52" class="sleepid_progress_bg"></circle>
              <circle cx="60" cy="60" r="52" class="sleepid_progress_path" data-sleepid-progress-path></circle>
            </svg>
            <div class="sleepid_progress_value" data-sleepid-progress-value>0%</div>
            <div class="sleepid_progress_pulse" aria-hidden="true"></div>
          </div>
        </div>
        <div class="sleepid_stage" data-sleepid-stage aria-live="polite" aria-atomic="true">
          <div class="sleepid_stage_panel sleepid_fade_content" data-stage="intro">
            <h2 class="sleepid_heading" id="sleepid-quiz-heading-{{ section.id }}">Begin your Sleepsona</h2>
            <div class="sleepid_credibility" data-sleepid-credibility>
              <span>{{ section.settings.credibility_text | escape }}</span>
            </div>
            <p class="sleepid_intro_text">Take a slow, guided journey to map your biological rhythm. We’ll breathe together, one question at a time.</p>
            <button type="button" class="sleepid_primary_btn" data-sleepid-start>
              <span>Enter the SleepID Quiz</span>
            </button>
          </div>
        </div>
      </div>
    </div>
    <div class="sleepid_gradual_blur sleepid_gradual_blur--top" aria-hidden="true"></div>
    <div class="sleepid_gradual_blur sleepid_gradual_blur--bottom" aria-hidden="true"></div>
  </div>
  <audio data-sleepid-chime src="{{ section.settings.chime_url | escape }}" preload="auto"></audio>
  <script type="application/json" id="sleepid-quiz-data-{{ section.id }}">
    {
      "colors": {
        "bg0": {{ section.settings.bg_0 | json }},
        "bg1": {{ section.settings.bg_1 | json }},
        "ink1": {{ section.settings.ink_1 | json }},
        "ink2": {{ section.settings.ink_2 | json }}
      },
      "branding": {
        "cardRadius": {{ section.settings.card_radius | default: 24 | json }},
        "buttonRadius": {{ section.settings.button_radius | default: 14 | json }},
        "shadowStrength": {{ section.settings.shadow_strength | default: "0 30px 60px rgba(0,0,0,0.08)" | json }},
        "enableAnimations": {{ section.settings.enable_animations | default: true | json }}
      },
      "credibilityText": {{ section.settings.credibility_text | json }},
      "analyzeLabel": {{ section.settings.analyze_button_label | json }},
      "analyzeDuration": {{ section.settings.analyze_duration_seconds | json }},
      "analyzePhrases": [
        {% assign phrases = '' %}
        {% if section.settings.analyze_phrase_1 != blank %}
          {% assign phrases = phrases | append: section.settings.analyze_phrase_1 | append: '||' %}
        {% endif %}
        {% if section.settings.analyze_phrase_2 != blank %}
          {% assign phrases = phrases | append: section.settings.analyze_phrase_2 | append: '||' %}
        {% endif %}
        {% if section.settings.analyze_phrase_3 != blank %}
          {% assign phrases = phrases | append: section.settings.analyze_phrase_3 | append: '||' %}
        {% endif %}
        {% assign phrase_array = phrases | split: '||' | reject: '' %}
        {% for phrase in phrase_array %}
          {{ phrase | strip | json }}{% unless forloop.last %},{% endunless %}
        {% endfor %}
      ],
      "features": {
        "progress": {{ section.settings.show_progress_indicator | default: true | json }},
        "autoShowSaved": {{ section.settings.auto_show_saved_report | json }},
        "transitionSpeed": {{ section.settings.transition_speed | default: 600 | json }}
      },
      "continueButtonLabel": {{ section.settings.continue_button_label | default: 'Continue to checkout' | json }},
      "questions": [
        {% assign question_blocks = section.blocks | where: 'type', 'question' %}
        {% for block in question_blocks %}
          {% assign option_strings = '' %}
          {% for n in (1..4) %}
            {% assign label_key = 'option_' | append: n | append: '_label' %}
            {% assign hint_key = 'option_' | append: n | append: '_hint' %}
            {% assign weight_key = 'option_' | append: n | append: '_weight' %}
            {% assign label_value = block.settings[label_key] %}
            {% if label_value != blank %}
              {% capture option_json %}
                {
                  "label": {{ label_value | json }},
                  "hint": {{ block.settings[hint_key] | json }},
                  "weight": {{ block.settings[weight_key] | default: 0 | plus: 0 }},
                  "optionId": "{{ block.id }}-{{ n }}"
                }
              {% endcapture %}
              {% assign option_json = option_json | strip %}
              {% if option_strings != '' %}
                {% assign option_strings = option_strings | append: ',' %}
              {% endif %}
              {% assign option_strings = option_strings | append: option_json %}
            {% endif %}
          {% endfor %}
          {
            "id": {{ block.settings.question_id | default: block.id | json }},
            "title": {{ block.settings.title | json }},
            "scienceContext": {{ block.settings.context | json }},
            "media": {
              "image": {% if block.settings.media_image != blank %}{{ block.settings.media_image | image_url: width: 1200 | json }}{% else %}null{% endif %},
              "alt": {{ block.settings.media_image_alt | json }}
            },
            "options": [{{ option_strings }}]
          }{% unless forloop.last %},{% endunless %}
        {% endfor %}
      ],
      "sleepTypes": [
        {% assign type_blocks = section.blocks | where: 'type', 'sleep_type' %}
        {% for block in type_blocks %}
          {% assign trait_strings = '' %}
          {% assign trait_lines = block.settings.traits_text | to_s | split: '\n' %}
          {% for trait in trait_lines %}
            {% assign trait_value = trait | strip %}
            {% if trait_value != '' %}
              {% if trait_strings != '' %}
                {% assign trait_strings = trait_strings | append: ',' %}
              {% endif %}
              {% assign trait_strings = trait_strings | append: trait_value | json %}
            {% endif %}
          {% endfor %}
          {% assign faq_strings = '' %}
          {% for n in (1..3) %}
            {% assign q_key = 'faq_' | append: n | append: '_question' %}
            {% assign a_key = 'faq_' | append: n | append: '_answer' %}
            {% assign q_val = block.settings[q_key] %}
            {% assign a_val = block.settings[a_key] %}
            {% if q_val != blank and a_val != blank %}
              {% capture faq_json %}
                {
                  "question": {{ q_val | json }},
                  "answer": {{ a_val | json }}
                }
              {% endcapture %}
              {% assign faq_json = faq_json | strip %}
              {% if faq_strings != '' %}
                {% assign faq_strings = faq_strings | append: ',' %}
              {% endif %}
              {% assign faq_strings = faq_strings | append: faq_json %}
            {% endif %}
          {% endfor %}
          {% assign products_json = '' %}
          {% for n in (1..4) %}
            {% assign product_key = 'bundle_product_' | append: n %}
            {% assign product_handle = block.settings[product_key] %}
            {% if product_handle != blank %}
              {% assign product_ref = all_products[product_handle] %}
              {% if product_ref %}
                {% capture product_json %}
                  {
                    "title": {{ product_ref.title | json }},
                    "url": {{ product_ref.url | json }},
                    "price": {{ product_ref.price | money | strip_html | json }},
                    "featuredImage": {% if product_ref.featured_image %}{{ product_ref.featured_image | image_url: width: 600 | json }}{% else %}null{% endif %},
                    "variantId": {% if product_ref.selected_or_first_available_variant %}{{ product_ref.selected_or_first_available_variant.id | json }}{% else %}null{% endif %},
                    "available": {{ product_ref.available | json }}
                  }
                {% endcapture %}
                {% assign product_json = product_json | strip %}
                {% if products_json != '' %}
                  {% assign products_json = products_json | append: ',' %}
                {% endif %}
                {% assign products_json = products_json | append: product_json %}
              {% endif %}
            {% endif %}
          {% endfor %}
          {
            "id": {{ block.settings.type_code | upcase | json }},
            "label": {{ block.settings.type_name | json }},
            "tagline": {{ block.settings.type_tagline | json }},
            "summary": {{ block.settings.summary | json }},
            "max": {{ block.settings.max_score | default: 20 | plus: 0 }},
            "scienceKey": {{ block.settings.science_key | json }},
            "image": {% if block.settings.result_image != blank %}{{ block.settings.result_image | image_url: width: 900 | json }}{% else %}null{% endif %},
            "report": {
              "overviewHeading": {{ block.settings.overview_heading | json }},
              "overviewBody": {{ block.settings.overview_body | json }},
              "traitsHeading": {{ block.settings.traits_heading | json }},
              "traits": [{{ trait_strings }}],
              "rhythmHeading": {{ block.settings.rhythm_heading | json }},
              "rhythmImage": {% if block.settings.rhythm_image != blank %}{{ block.settings.rhythm_image | image_url: width: 1400 | json }}{% else %}null{% endif %},
              "rhythmCaption": {{ block.settings.rhythm_caption | json }},
              "protocolHeading": {{ block.settings.protocol_heading | json }},
              "protocolMorning": {{ block.settings.protocol_morning | json }},
              "protocolEvening": {{ block.settings.protocol_evening | json }},
              "bundleHeading": {{ block.settings.bundle_heading | json }},
              "bundleDescription": {{ block.settings.bundle_description | json }},
              "products": [{{ products_json }}],
              "faqHeading": {{ block.settings.faq_heading | json }},
              "faq": [{{ faq_strings }}],
              "footerNote": {{ block.settings.footer_note | json }}
            }
          }{% unless forloop.last %},{% endunless %}
        {% endfor %}
      ],
      "scienceContexts": {
        {% assign context_blocks = section.blocks | where: 'type', 'science_context' %}
        {% for block in context_blocks %}
          {{ block.settings.question_id | json }}: {{ block.settings.science_context | json }}{% unless forloop.last %},{% endunless %}
        {% endfor %}
      }
    }
  </script>
</section>
<style>
  #sleepid-quiz-section-{{ section.id }} {
    --sleepid-bg-0: {{ section.settings.bg_0 }};
    --sleepid-bg-1: {{ section.settings.bg_1 }};
    --sleepid-ink-1: {{ section.settings.ink_1 }};
    --sleepid-ink-2: {{ section.settings.ink_2 }};
    --sleepid-accent: #eae4dc;
    --sleepid-muted: rgba(255,255,255,0.75);
    --sleepid-border: rgba(0,0,0,0.06);
    --sleepid-shadow: {{ section.settings.shadow_strength | default: '0 30px 60px rgba(0,0,0,0.08)' }};
    --sleepid-card-radius: {{ section.settings.card_radius | default: 24 }}px;
    --sleepid-btn-radius: {{ section.settings.button_radius | default: 14 }}px;
    --sleepid-progress-size: 120px;
    --sleepid-thread-color: rgba(152,160,166,0.08);
    --sleepid-thread-glow: rgba(234,228,220,0.35);
    font-family: "Inter", "Manrope", "Segoe UI", system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
    color: var(--sleepid-ink-1);
    position: relative;
    overflow: hidden;
     transition: transform 0.6s ease, opacity 0.6s ease;
    --sleepid-fullscreen-enter-y: 0px;
  }
  body[data-sleepid-quiz-open] {
    overflow: hidden;
  }
  #sleepid-quiz-section-{{ section.id }}[data-fullscreen="true"] {
    position: fixed;
    inset: 0;
    margin: 0 !important;
    width: 100%;
    height: 100%;
    min-height: 100vh;
    min-height: 100dvh;
    z-index: 9998;
    overflow-y: auto;
    background: linear-gradient(140deg, var(--sleepid-bg-0), var(--sleepid-bg-1));
    padding: 0;
    transform: translate3d(0, var(--sleepid-fullscreen-enter-y), 0);
  }
  #sleepid-quiz-section-{{ section.id }}[data-fullscreen="true"][data-fullscreen-state="settled"] {
    transform: translate3d(0, 0, 0);
  }
  #sleepid-quiz-section-{{ section.id }}[data-fullscreen="true"] .sleepid_section_surface {
    min-height: 100vh;
    min-height: 100dvh;
    padding: clamp(48px, 6vw, 96px) clamp(24px, 6vw, 72px);
    align-items: center;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_quiz_card {
    transition: transform 0.6s ease, box-shadow 0.6s ease;
    transform-origin: center center;
    will-change: transform, box-shadow;
  }
  #sleepid-quiz-section-{{ section.id }}[data-fullscreen="true"] .sleepid_quiz_card {
    width: min(88vw, 760px);
    box-shadow: 0 40px 80px rgba(0,0,0,0.18);
    transform: scale(1.02);
  }
  #sleepid-quiz-section-{{ section.id }}[data-fullscreen="true"] .sleepid_card_inner {
    overflow-y: auto;
    min-height: clamp(420px, 70vh, 720px);
    max-height: calc(100dvh - clamp(80px, 12vh, 180px));
    scrollbar-gutter: stable both-edges;
  }
  #sleepid-quiz-section-{{ section.id }}[data-fullscreen="true"] .sleepid_gradual_blur {
    display: none;
  }
  #sleepid-quiz-section-{{ section.id }} *,
  #sleepid-quiz-section-{{ section.id }} *::before,
  #sleepid-quiz-section-{{ section.id }} *::after {
    box-sizing: border-box;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_section_surface {
    position: relative;
    padding: clamp(48px, 8vw, 80px) 0;
    display: flex;
    justify-content: center;
    background: linear-gradient(140deg, var(--sleepid-bg-0), var(--sleepid-bg-1));
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_threads {
    position: absolute;
    inset: 0;
    background:
      radial-gradient(circle at 20% 20%, rgba(255,255,255,0.3), transparent 55%),
      radial-gradient(circle at 80% 80%, rgba(255,255,255,0.25), transparent 60%);
    mix-blend-mode: screen;
    overflow: hidden;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_threads::before,
  #sleepid-quiz-section-{{ section.id }} .sleepid_threads::after {
    content: '';
    position: absolute;
    inset: -120px;
    background: repeating-linear-gradient(
      115deg,
      transparent 0 40px,
      var(--sleepid-thread-color) 40px 44px,
      transparent 44px 92px
    );
    opacity: 0.65;
    animation: sleepidThreadsDrift 16s linear infinite;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_threads::after {
    animation-duration: 22s;
    animation-direction: reverse;
    opacity: 0.35;
  }
  @keyframes sleepidThreadsDrift {
    0% { transform: translateX(-6%); }
    50% { transform: translateX(6%); }
    100% { transform: translateX(-6%); }
  }
  @media (prefers-reduced-motion: reduce) {
      #sleepid-quiz-section-{{ section.id }} {
      transition: none;
    }
    #sleepid-quiz-section-{{ section.id }} .sleepid_quiz_card {
      transition: none;
    }
    #sleepid-quiz-section-{{ section.id }} .sleepid_threads::before,
    #sleepid-quiz-section-{{ section.id }} .sleepid_threads::after {
      animation: none;
    }
    #sleepid-quiz-section-{{ section.id }} .sleepid_logo,
    #sleepid-quiz-section-{{ section.id }} .sleepid_logo_shimmer,
    #sleepid-quiz-section-{{ section.id }} .sleepid_progress_value,
    #sleepid-quiz-section-{{ section.id }} .sleepid_progress_pulse,
    #sleepid-quiz-section-{{ section.id }} .sleepid_primary_btn::after,
    #sleepid-quiz-section-{{ section.id }} .sleepid_heading,
    #sleepid-quiz-section-{{ section.id }} .sleepid_fade_content,
    :is(#sleepid-quiz-section-{{ section.id }}, .sleepid_result_cinematic_overlay) .sleepid_scene,
    :is(#sleepid-quiz-section-{{ section.id }}, .sleepid_result_cinematic_overlay) .sleepid_scene_reveal,
    :is(#sleepid-quiz-section-{{ section.id }}, .sleepid_result_cinematic_overlay) .sleepid_result_cinematic,
    :is(#sleepid-quiz-section-{{ section.id }}, .sleepid_result_cinematic_overlay) .sleepid_cinematic_ambient,
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_summary_shell,
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_product {
      animation: none;
    }
    :is(#sleepid-quiz-section-{{ section.id }}, .sleepid_result_cinematic_overlay) .sleepid_scene,
    :is(#sleepid-quiz-section-{{ section.id }}, .sleepid_result_cinematic_overlay) .sleepid_result_cinematic,
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_summary_shell,
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_visual,
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_product {
      transition: none;
    }
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_product::before,
    #sleepid-quiz-section-{{ section.id }} .sleepid_result_product::after {
      animation: none !important;
    }
    #sleepid-quiz-section-{{ section.id }} .sleepid_fade_content {
      opacity: 1;
      transform: none;
      filter: none;
    }
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_fingerprint {
    position: absolute;
    inset: 0;
    background-image:
      radial-gradient(circle at 50% 12%, rgba(152,160,166,0.12) 0, transparent 55%),
      radial-gradient(circle at 50% 90%, rgba(152,160,166,0.08) 0, transparent 65%),
      repeating-radial-gradient(circle at 50% 30%, rgba(152,160,166,0.08) 0 1px, transparent 1px 12px);
    opacity: 0.55;
    mix-blend-mode: soft-light;
    pointer-events: none;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_quiz_card {
    position: relative;
    width: min(92vw, 720px);
    padding: 2px;
    border-radius: var(--sleepid-card-radius);
    background: linear-gradient(140deg, rgba(255,255,255,0.4), rgba(255,255,255,0));
    box-shadow: 0 20px 40px rgba(0,0,0,0.08);
    z-index: 1;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_card_shimmer {
    position: absolute;
    inset: 0;
    border-radius: inherit;
    background: radial-gradient(circle at 10% 10%, rgba(255,255,255,0.4), transparent 60%),
      linear-gradient(120deg, rgba(255,255,255,0.2), rgba(255,255,255,0));
    opacity: 0.75;
    filter: blur(0.5px);
    pointer-events: none;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_card_inner {
    position: relative;
    background: rgba(255,255,255,0.9);
    border-radius: calc(var(--sleepid-card-radius) - 2px);
    border: 1px solid var(--sleepid-border);
    box-shadow: var(--sleepid-shadow);
    padding: clamp(24px, 5vw, 36px);
    display: grid;
    gap: clamp(20px, 3vw, 28px);
    overflow: hidden;
    min-height: 520px;
  }
  #sleepid-quiz-section-{{ section.id }} .sleepid_close_btn {
    position: absolute;
    top: clamp(12px, 3vw, 24px);
    right: clamp(12px, 3vw, 24px);
    width: 40px;
    height: 40px;
    border-radius: 50%;
    border: 1px solid rgba(0,0,0,0.08);
    background: rgba(255,255,255,0.86);
    color: var(--sleepid-ink-1);
