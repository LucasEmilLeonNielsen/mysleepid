    function handleStart(force) {
      if (force && typeof force.preventDefault === 'function') {
        force.preventDefault();
        force = undefined;
      }
      enterFullscreenExperience();
      goToQuestion(0);
    }

    if (startBtn) {
      startBtn.addEventListener('click', handleStart);
    }

