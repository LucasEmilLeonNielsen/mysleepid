      if (!storedResultId) {
        return false;
      }
      const normalized = storedResultId.toString().trim().toUpperCase();
      if (!normalized || normalized === 'NULL' || normalized === 'UNDEFINED') {
        return false;
      }
      const storedEntry = resultMap.find(function(entry) {
