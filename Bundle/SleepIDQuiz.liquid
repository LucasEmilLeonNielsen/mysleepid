      { max: 20, id: 'D', label: 'Eclipse Pioneer', summary: 'Nightfall awakens your insight. Protect late creativity with restorative buffers.', scienceKey: 'night' },
      { max: Infinity, id: 'E', label: 'Nebula Nocturne', summary: 'You thrive when the world softens. Embrace moonlit productivity with mindful recovery cues.', scienceKey: 'night' }
    ];

    const sleepTypeExtras = {};
    const sleepTypeEntries = Array.isArray(config.sleepTypes)
      ? config.sleepTypes.filter(function(entry) {
          return entry && entry.id;
        })
      : [];
    const activeSleepTypes = sleepTypeEntries.length ? sleepTypeEntries : defaultSleepTypes;
    const resultMap = activeSleepTypes.map(function(entry, index) {
      const typeId = (entry.id || '').toString().replace(/[^a-z0-9]/gi, '').toUpperCase() || String.fromCharCode(65 + index);
      const maxScore = Number(entry.max);
      const max = Number.isFinite(maxScore) ? maxScore : Infinity;
      const label = entry.label || typeId;
      const summary = entry.summary || '';
      if (entry.image) {
        productImages[typeId] = entry.image;
      }
      sleepTypeExtras[typeId] = {
        label: label,
        summary: summary,
        tagline: entry.tagline || '',
        image: entry.image || '',
        cta: entry.cta || {},
        scienceKey: entry.scienceKey || fallbackScienceKeyMap[typeId] || '',
        report: entry.report || {}
      };
      if (!sleepTypeExtras[typeId].scienceKey) {
        sleepTypeExtras[typeId].scienceKey = fallbackScienceKeyMap[typeId] || '';
      }
      return {
        max: max,
        id: typeId,
        label: label,
        summary: summary
      };
    });
    const fallbackProductImages = {
      A: '{{ section.settings.result_image_aurora | image_url }}',
      B: '{{ section.settings.result_image_day | image_url }}',
      C: '{{ section.settings.result_image_lumen | image_url }}',
      D: '{{ section.settings.result_image_eclipse | image_url }}',
      E: '{{ section.settings.result_image_night | image_url }}'
    };
    Object.keys(fallbackProductImages).forEach(function(key) {
      const value = (fallbackProductImages[key] || '').trim();
      if (value && !productImages[key]) {
        productImages[key] = value;
      }
    });

    function emitEvent(name, detail) {
      if (!name || typeof window.CustomEvent !== 'function') return;
      try {
        window.dispatchEvent(new CustomEvent(name, { detail: detail || {} }));
      } catch (error) {}
    }

    function getSleepTypeData(typeId) {
      if (!typeId) return null;
      const normalized = typeId.toString().trim().toUpperCase();
      return sleepTypeExtras[normalized] || null;
    }

    function resolveScienceKey(typeId) {
      const entry = getSleepTypeData(typeId);
      if (entry && entry.scienceKey) {
        return entry.scienceKey;
      }
      return fallbackScienceKeyMap[typeId] || '';
    }

    function createHtmlContainer(tag, className, text) {
      const node = document.createElement(tag);
      if (className) {
        node.className = className;
      }
      if (typeof text === 'string') {
        node.textContent = text;
      }
      return node;
    }

    function appendRichContent(parent, content) {
      if (!parent || !content) return;
      const wrapper = document.createElement('div');
      wrapper.innerHTML = content;
      parent.appendChild(wrapper);
    }

    function buildListFromText(content) {
      if (!content) return null;
      const trimmed = content.trim();
      if (!trimmed) return null;
      const hasMarkup = /<[a-z][\s\S]*>/i.test(trimmed);
      if (hasMarkup) {
        const wrapper = document.createElement('div');
        wrapper.innerHTML = trimmed;
        return wrapper;
      }
      const list = document.createElement('ul');
      trimmed.split(/\r?\n/).map(function(line) {
        return line.trim();
      }).filter(Boolean).forEach(function(line) {
        const li = document.createElement('li');
        li.textContent = line;
        list.appendChild(li);
      });
      return list;
    }

    function addVariantToCart(variantId, button) {
      if (!variantId || !button || button.dataset.state === 'loading') {
        return;
      }
      const originalText = button.textContent;
      const rootRoutes = window.Shopify && window.Shopify.routes ? window.Shopify.routes : {};
      const baseRoot = rootRoutes.root || rootRoutes.root_url || '';
      const endpoint = baseRoot.replace(/\/$/, '') + '/cart/add.js';
      button.dataset.state = 'loading';
      button.disabled = true;
      button.textContent = 'Adding…';
      fetch(endpoint, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
          Accept: 'application/json',
          'X-Requested-With': 'XMLHttpRequest'
        },
        credentials: 'same-origin',
        body: JSON.stringify({
          items: [
            {
              id: variantId,
              quantity: 1
            }
          ]
        })
      }).then(function(response) {
        if (!response.ok) {
          throw new Error('Request failed');
        }
        return response.json();
      }).then(function() {
        button.dataset.state = 'success';
        button.textContent = 'Added!';
        emitEvent('sleepid_product_added', {
          variantId: variantId,
          type: resultType
        });
        window.setTimeout(function() {
          button.dataset.state = '';
          button.disabled = false;
          button.textContent = originalText;
        }, 1800);
      }).catch(function() {
        button.dataset.state = 'error';
        button.textContent = 'Try again';
        window.setTimeout(function() {
          button.dataset.state = '';
          button.disabled = false;
          button.textContent = originalText;
        }, 2200);
      });
    }

    function buildProductCard(product, options) {
      if (!product) return null;
      const card = document.createElement('div');
      const baseClass = options && options.baseClass ? options.baseClass : 'sleepid_product_card';
      card.className = baseClass;
      if (product.featuredImage) {
        const img = document.createElement('img');
        img.src = product.featuredImage;
        img.alt = product.title || 'Product image';
        card.appendChild(img);
      }
      if (product.title) {
        if (product.url) {
          const link = document.createElement('a');
          link.href = product.url;
          link.textContent = product.title;
          link.className = 'sleepid_product_card_title';
          card.appendChild(link);
        } else {
          const titleEl = createHtmlContainer('p', 'sleepid_product_card_title', product.title);
          card.appendChild(titleEl);
        }
      }
      if (product.price) {
        const priceEl = createHtmlContainer('p', 'sleepid_product_price', product.price);
        card.appendChild(priceEl);
      }
      if (product.variantId) {
        const button = document.createElement('button');
        button.type = 'button';
        button.className = options && options.buttonClass ? options.buttonClass : 'sleepid_primary_btn sleepid_product_add_btn';
        button.textContent = (options && options.buttonLabel) || 'Add to cart';
        button.dataset.variantId = product.variantId;
        if (product.available === false) {
          button.disabled = true;
          button.textContent = 'Sold out';
        }
        button.addEventListener('click', function() {
          addVariantToCart(product.variantId, button);
        });
        card.appendChild(button);
      }
      return card;
    }

