      if (event.key === 'Escape') {
        const isFullscreen = rootEl.getAttribute('data-fullscreen') === 'true';
        if (isFullscreen || (currentIndex > -1 && currentIndex < questions.length)) {
          event.preventDefault();
          resetQuiz();
        }
