      : [];
    if (!questions.length) {
      questions = defaultQuestions.map(function(question) {
        return Object.assign({}, question);
      });
    }

    questions.forEach(function(question, index) {
      const mapped = scienceContextMap[question.id];
      const trimmed = typeof mapped === 'string' ? mapped.trim() : '';
      if (!question.scienceContext || !question.scienceContext.length) {
        question.scienceContext = trimmed || question.context || '';
      }
      if (Object.prototype.hasOwnProperty.call(question, 'context')) {
        delete question.context;
      }
      question.index = index;
    });
    const questionAdvanceDelay = Math.max(260, Math.min(900, transitionSpeedMs));

    const defaultSleepTypes = [
      { max: 9, id: 'A', label: 'Aurora Sync', summary: 'Your energy brightens with first light. A structured dawn ritual keeps your Sleepsona harmonised.', scienceKey: 'day' },
